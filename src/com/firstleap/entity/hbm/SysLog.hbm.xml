<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.firstleap.entity.po.SysLog" table="SYS_LOG_DATETIME">
        <id name="logId" type="java.lang.String">
            <column name="LOG_ID" length="32" />
            <generator class="assigned" />
        </id>
        <property name="operator" type="java.lang.String">
            <column name="OPERATOR" length="100" />
        </property>
        <property name="operatFunc" type="java.lang.String">
            <column name="OPERAT_FUNC" length="100" />
        </property>
        <property name="operatObject" type="java.lang.String">
            <column name="OPERAT_OBJECT" length="100" />
        </property>
        <property name="operatContent" type="java.lang.String">
            <column name="OPERAT_CONTENT" length="100" />
        </property>
        <property name="operatTime" type="java.util.Date">
            <column name="OPERAT_TIME" length="7" />
        </property>
    </class>
</hibernate-mapping>
